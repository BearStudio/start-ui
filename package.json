{
  "name": "start-ui-web",
  "private": true,
  "version": "1.0.0",
  "description": "Opinionated UI starter with ⚛️ React, ⚡️ Chakra UI, ⚛️ React Query & 🐜 Formiz — From the 🐻 BearStudio Team",
  "engines": {
    "node": ">=16.8.0 <19.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "prisma generate && yarn build:info && yarn theme:generate-typing",
    "test": "cypress open",
    "test:ci": "SKIP_ENV_VALIDATIONS=true cypress run --component",
    "dev": "next dev",
    "build": "yarn build:info && next build",
    "build:info": "node .build-info.generate.js",
    "start": "next start",
    "pretty": "prettier -w .",
    "lint": "SKIP_ENV_VALIDATIONS=true next lint && tsc --noEmit",
    "storybook": "yarn build:info && storybook dev --no-open -p 6006",
    "storybook:build": "yarn build:info && storybook build && mv ./storybook-static ./public/storybook",
    "theme:generate-typing": "chakra-cli tokens ./src/theme/theme.ts",
    "theme:generate-icons": "svgr --config-file src/components/Icons/svgr.config.js src/components/Icons/svg-sources",
    "db:push": "prisma db push",
    "db:ui": "prisma studio",
    "db:seed": "prisma db seed"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write"
  },
  "dependencies": {
    "@chakra-ui/next-js": "2.1.4",
    "@chakra-ui/react": "2.7.0",
    "@chakra-ui/theme-tools": "2.0.18",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@faker-js/faker": "8.0.2",
    "@fontsource/inter": "5.0.2",
    "@formiz/core": "2.0.0-beta.12",
    "@formiz/validations": "2.0.0-beta.2",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@lukemorales/query-key-factory": "1.2.0",
    "@prisma/client": "4.15.0",
    "@tanstack/react-query": "4.35.3",
    "@tanstack/react-query-devtools": "4.35.3",
    "@trpc/client": "10.38.2",
    "@trpc/react-query": "10.38.2",
    "@trpc/server": "10.38.2",
    "bcrypt": "5.1.1",
    "dayjs": "1.11.8",
    "framer-motion": "10.12.16",
    "i18next": "22.5.0",
    "isomorphic-form-data": "2.0.0",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "next": "13.4.19",
    "nodemailer": "6.9.5",
    "react": "18.2.0",
    "react-currency-input-field": "3.6.10",
    "react-day-picker": "7.4.10",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "react-i18next": "12.3.1",
    "react-icons": "4.9.0",
    "react-popper": "2.3.0",
    "react-select": "5.7.3",
    "react-use-measure": "2.1.1",
    "sharp": "0.32.1",
    "superjson": "1.13.1",
    "zod": "3.22.2",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/parser": "7.22.4",
    "@chakra-ui/cli": "2.4.1",
    "@next/eslint-plugin-next": "13.4.19",
    "@storybook/addon-actions": "7.4.1",
    "@storybook/addon-essentials": "7.4.1",
    "@storybook/addon-links": "7.4.1",
    "@storybook/addon-mdx-gfm": "7.4.1",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/nextjs": "7.4.1",
    "@storybook/react": "7.4.1",
    "@storybook/theming": "7.4.1",
    "@svgr/cli": "8.0.1",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/bcrypt": "5.0.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.2.5",
    "@types/nodemailer": "6.4.10",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "babel-eslint": "10.1.0",
    "cypress": "12.13.0",
    "encoding": "0.1.13",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "storybook": "7.4.1",
    "storybook-dark-mode": "3.0.1",
    "tsx": "3.12.7",
    "typescript": "5.1.3"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
