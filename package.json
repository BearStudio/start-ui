    "nextjs-toploader": "1.6.4",
    "nodemailer": "6.9.7",
    "pino": "8.16.1",
    "pino-pretty": "10.2.3",
    "react": "18.2.0",
    "react-currency-input-field": "3.6.11",
    "react-day-picker": "7.4.10",
