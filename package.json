    "lodash": "4.17.21",
    "next": "14.1.0",
    "nodemailer": "6.9.8",
    "nuqs": "1.16.0",
    "pino": "8.17.2",
    "pino-pretty": "10.3.1",
    "react": "18.2.0",
    "react-currency-input-field": "3.6.14",
    "react-day-picker": "8.10.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
    "react-focus-lock": "2.9.7",
    "react-i18next": "14.0.1",
